<h1>Edit Team Information</h1>
<br>
<%=form_for :team, url: team_path(@team), method: :patch do |f| %>
   <p>
      <%= f.label :name %><br>
      <%= f.text_field :name %><br>
   </p>
   
   <p>
      <%= f.label :tournament_id %><br>
      <%= f.text_field :tournament_id %><br>
   </p>

   <p>
      <%= f.submit %>
   </p>
<% end %>

<%= link_to 'Back', team_path(@team) %>